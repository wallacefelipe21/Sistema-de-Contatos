@{
    ViewData["Title"] = "Editar Contato";
}

<div class="text-center">

    <h1 class="display-4">Editar Contato</h1>

</div>

<form>
    <div class="form-group">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o Nome">
    </div>

    <br />

    <div class="form-group">
        <label for="nome">Email</label>
        <input type="text" class="form-control" placeholder="Digite o Email ">
    </div>

    <br />

    <div class="form-group">
        <label for="nome">Telefone</label>
        <input type="text" class="form-control" placeholder="Digite o Telefone">
    </div>

    <br />

    <button type="submit" class="btn btn-primary">Alterar</button>
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>
